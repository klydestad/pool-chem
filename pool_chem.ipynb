    "# Import reader module from csv Library\n",
    "from csv import reader\n",
    "\n",
    "# read the CSV file\n",
    "def load_csv(filename):\n",
    "    # Open file in read mode\n",
    "    file = open(filename,\"r\")\n",
    "    # Reading file \n",
    "    lines = reader(file)\n",
    "    \n",
    "    # Converting into a list \n",
    "    data = list(lines)\n",
    "    return data\n",
    "\n",
    "if __name__ == \"__main__\":\n",
